---
import Layout from '#layouts/Layout.astro'
import Prose from '#components/prose.astro'
import SignOut from '#components/sign-out.astro'

export const prerender = false

const {user} = Astro.locals
---

<Layout>
	<Prose as="main">
		<h1>
			Hello {user.firstName}!
		</h1>
		<SignOut />
	</Prose>
</Layout>
